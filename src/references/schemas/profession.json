{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "App",
  "type": "object",
  "properties": {
    "gid": {
      "type": "string",
      "readOnly": true
    },
    "href": {
      "type": "string",
      "readOnly": true
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    },
    "modified_at": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    },
    "name": {
      "type": "string",
      "maxLength": 128,
      "required": true
    },
    "slug": {
      "type": "string",
      "maxLength": 128,
      "required": true
    }
  },
  "additionalProperties": false
}
