{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "properties": {
    "gid": {
      "type": "string",
      "readOnly": true
    },
    "href": {
      "type": "string",
      "readOnly": true
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    },
    "modified_at": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    },
    "name": {
      "type": "string",
      "maxLength": 80,
      "required": true
    },
    "domain": {
      "type": [null, "string"]
    },
    "hostname": {
      "type": [null, "string"]
    },
    "vat": {
      "type": [null, "string"],
      "minLength": 5,
      "maxLength": 17,
      "required": false
    },
    "billing_address": {
      "$ref": "#/definitions/address",
      "required": true
    },
    "billing_contact": {
      "$ref": "#/definitions/billing_contact",
      "required": false
    },
    "logo": {
      "oneOf": [
          {"type": "null"},
          {"$ref": "image.json"}
      ],
      "required": false
    },
    "additional_meta" : {
      "type": ["object","array"]
    },
    "tax_inclusive": {
      "type": "boolean"
    },
    "parent": {
      "oneOf": [
          {"type": "null"},
          {"$ref": "reference.json"}
      ],
      "required": false
    },
    "spreadsheet_id": {
      "type": [null, "string"],
      "readOnly": true
    },
    "is_deleted": {
      "type": "boolean",
      "required": false
    },
    "email_enabled": {
      "type": "boolean",
      "required": false
    },
    "owner": {
      "$ref": "user.reference.json",
      "required": true
    },
    "test": {
      "type": "boolean",
      "required": false
    },
    "hubspot_id": {
      "type": [null, "integer"],
      "required": false
    },
    "sageone_contact_url": {
        "type": [null, "string"],
        "readOnly": true
    },
    "hubspot_company_url": {
        "type": [null, "string"],
        "readOnly": true
    },
    "subscription_price": {
        "$ref": "price_value.embed.json",
        "required": false
    },
    "is_facebook_enabled": {
      "type": "boolean",
      "required": false
    }
  },
  "definitions": {
    "address": {
      "properties": {
        "division": {
          "type": [null, "string"],
          "maxLength": 80,
          "required": false
        },
        "address1": {
          "type": [null, "string"],
          "maxLength": 80,
          "required": true
        },
        "address2": {
          "type": [null, "string"],
          "maxLength": 80,
          "required": false
        },
        "zipcode": {
          "type": "string",
          "maxLength": 10,
          "required": true
        },
        "city": {
          "type": "string",
          "maxLength": 80,
          "required": true
        },
        "state": {
          "type": [null, "string"],
          "maxLength": 80,
          "required": false
        },
        "country": {
          "type": "string",
          "maxLength": 2,
          "required": true
        },
        "country_name": {
          "type": "string",
          "readOnly": true
        },
        "additional": {
          "type": [null, "string"],
          "maxLength": 80,
          "required": false
        }
      },
      "additionalProperties": false
    },
    "billing_contact": {
      "properties": {
        "phone": {
          "type": [null, "string"],
          "maxLength": 80,
          "required": false
        },
        "email": {
          "type": [null, "string"],
          "maxLength": 80,
          "required": false
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
