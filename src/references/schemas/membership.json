{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "properties": {
    "gid": {
      "type": "string",
      "readOnly": true
    },
    "href": {
      "type": "string",
      "readOnly": true
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    },
    "modified_at": {
      "type": "string",
      "format": "date-time",
      "readOnly": true
    },
    "is_manager": {
      "type": "boolean",
      "required": false
    },
    "monthly_budget": {
      "oneOf": [
        {"type": "null"},
        {"$ref": "price_value.embed.json"}
      ],
      "required": false
    },
    "monthly_spent": {
        "$ref": "monthly_spent.embed.json",
        "readOnly": true
    },
    "organization": {
      "$ref": "reference.json",
      "required": false
    },
    "user": {
      "$ref": "user.reference.json",
      "required": false,
      "readOnly": true
    }
  },
  "additionalProperties": false
}
