{
  "name": "partners-doc",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "ln -sf docs/ partners-doc 2>/dev/null",
    "start": "node index.js",
    "prebuild": "npm i",
    "build": "node build.js",
    "version": "npm run build && git add -A docs",
