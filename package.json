    "Samuel Vaillant <s.vaillant@creads.org>",
    "Damien Pitard <d.pitard@creads.org>"
  ],
  "dependencies": {
    "async": "^1.5.2",
    "debug": "^2.6.9",
    "marked": "^0.6.1",
    "merge": "^1.2.1",
    "metalsmith": "^2.3.0",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-headings-identifier": "^0.0.11",
    "metalsmith-ignore": "^1.0.0",
    "metalsmith-layouts": "^1.8.1",
    "metalsmith-markdown": "^1.2.0",
    "metalsmith-metadata": "^0.0.4",
    "metalsmith-permalinks": "^2.2.0",
    "metalsmith-prism": "^3.1.1",
    "minimize": "^2.2.0",
    "nunjucks": "^3.1.7",
    "nunjucks-markdown": "^1.0.0",
    "q": "^1.5.1",
